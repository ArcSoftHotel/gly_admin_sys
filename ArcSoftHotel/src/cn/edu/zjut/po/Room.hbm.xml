<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD
3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.edu.zjut.po.Room" table="roominfo" catalog="ArcSoftHotel">
        <id name="roomid" type="int">
			<column name="roomid" unique="true" />
		</id>
		<property name="roomtype" type="string">
			<column name="roomtype" length="10" />
		</property>
		<property name="roomcost" type="string">
			<column name="roomcost" length="10" />
		</property>
    </class>
</hibernate-mapping>